{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "cd ../.. && npm install",
  "outputDirectory": "build/client",
  "framework": null,
  "functions": {
    "api/index.js": {
      "includeFiles": "build/**/*",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1",
    "ALLOWED_ORIGINS": "https://dso-signtusk.vercel.app,https://*.vercel.app"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "VERCEL": "1",
      "SKIP_ENV_VALIDATION": "true",
      "TURBO_TELEMETRY_DISABLED": "1",
      "SKIP_PATCHES": "true"
    }
  }
}
