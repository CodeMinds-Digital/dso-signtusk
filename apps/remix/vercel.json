{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["build/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server/main.js"
    }
  ],
  "env": {
    "SKIP_PATCHES": "true",
    "INSTALL_PLAYWRIGHT": "false",
    "NODE_ENV": "production"
  }
}