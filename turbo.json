{
  "$schema": "https://turbo.build/schema.json",
  "remoteCache": {
    "signature": true
  },
  "experimentalSpaces": {
    "id": "signtusk-vercel"
  },
  "tasks": {
    "build": {
      "dependsOn": ["prebuild", "@signtusk/prisma#build", "^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        "npm/**",
        "*.node"
      ],
      "inputs": [
        "src/**",
        "app/**",
        "pages/**",
        "components/**",
        "lib/**",
        "styles/**",
        "public/**",
        "package.json",
        "next.config.js",
        "remix.config.js",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_APP_URL",
        "NEXT_PUBLIC_WEBAPP_URL",
        "NEXT_PUBLIC_PROJECT"
      ]
    },
    "build:vercel": {
      "dependsOn": ["prebuild", "@signtusk/prisma#build:vercel", "^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        "npm/**",
        "*.node"
      ],
      "inputs": [
        "src/**",
        "app/**",
        "pages/**",
        "components/**",
        "lib/**",
        "styles/**",
        "public/**",
        "package.json",
        "next.config.js",
        "remix.config.js",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "env": [
        "NODE_ENV",
        "SKIP_ENV_VALIDATION",
        "CARGO_TARGET_DIR",
        "CARGO_BUILD_TARGET",
        "RUSTFLAGS",
        "NEXT_PUBLIC_APP_URL",
        "NEXT_PUBLIC_WEBAPP_URL",
        "NEXT_PUBLIC_PROJECT",
        "NEXT_PUBLIC_FEATURE_BILLING_ENABLED",
        "NEXT_PUBLIC_DISABLE_SIGNUP",
        "NEXT_PUBLIC_DOCUMENT_SIZE_UPLOAD_LIMIT",
        "NEXT_PUBLIC_UPLOAD_TRANSPORT",
        "NEXT_PUBLIC_POSTHOG_KEY",
        "NEXT_PUBLIC_POSTHOG_HOST",
        "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
        "NEXT_PUBLIC_MARKETING_URL",
        "NEXT_PUBLIC_DOCS_URL"
      ],
      "cache": true,
      "passThroughEnv": [
        "VERCEL",
        "VERCEL_ENV",
        "VERCEL_URL",
        "VERCEL_BRANCH_URL",
        "VERCEL_REGION"
      ]
    },
    "@signtusk/prisma#build": {
      "outputs": [
        "generated/**",
        "../../node_modules/@prisma/client/**",
        "types.ts"
      ],
      "inputs": ["schema.prisma", "migrations/**", "package.json"],
      "env": [
        "DATABASE_URL",
        "NEXT_PRIVATE_DATABASE_URL",
        "NEXT_PRIVATE_DIRECT_DATABASE_URL",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "DATABASE_URL_UNPOOLED",
        "NODE_ENV"
      ]
    },
    "@signtusk/prisma#build:vercel": {
      "outputs": [
        "generated/**",
        "../../node_modules/@prisma/client/**",
        "types.ts"
      ],
      "inputs": [
        "schema.prisma",
        "migrations/**",
        "package.json",
        "scripts/vercel-generate.js"
      ],
      "env": [
        "DATABASE_URL",
        "NEXT_PRIVATE_DATABASE_URL",
        "NEXT_PRIVATE_DIRECT_DATABASE_URL",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "DATABASE_URL_UNPOOLED",
        "NODE_ENV",
        "CHECKPOINT_DISABLE"
      ]
    },
    "@signtusk/prisma#prebuild": {
      "outputs": [
        "generated/**",
        "../../node_modules/@prisma/client/**",
        "types.ts"
      ],
      "inputs": ["schema.prisma", "migrations/**", "package.json"],
      "env": [
        "DATABASE_URL",
        "NEXT_PRIVATE_DATABASE_URL",
        "NEXT_PRIVATE_DIRECT_DATABASE_URL",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "DATABASE_URL_UNPOOLED",
        "NODE_ENV"
      ]
    },
    "@signtusk/prisma#prebuild:vercel": {
      "outputs": [
        "generated/**",
        "../../node_modules/@prisma/client/**",
        "types.ts"
      ],
      "inputs": [
        "schema.prisma",
        "migrations/**",
        "package.json",
        "scripts/vercel-generate.js"
      ],
      "env": [
        "DATABASE_URL",
        "NEXT_PRIVATE_DATABASE_URL",
        "NEXT_PRIVATE_DIRECT_DATABASE_URL",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "DATABASE_URL_UNPOOLED",
        "NODE_ENV",
        "CHECKPOINT_DISABLE"
      ]
    },
    "prebuild": {
      "dependsOn": ["@signtusk/prisma#prebuild:vercel", "^prebuild"],
      "inputs": ["package.json", "tsconfig.json"]
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:debug": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "type-check": {
      "inputs": [
        "src/**",
        "app/**",
        "components/**",
        "lib/**",
        "package.json",
        "tsconfig.json"
      ],
      "env": ["NODE_ENV"]
    },
    "db:migrate": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "cache:clear": {
      "cache": false
    },
    "sitemap:generate": {
      "cache": false
    }
  },
  "globalDependencies": [
    "**/.env.*local",
    ".env",
    ".env.local",
    ".env.production",
    ".env.development",
    "vercel.json"
  ],
  "globalEnv": [
    "APP_VERSION",
    "AWS_ACCESS_KEY_ID",
    "AWS_REGION",
    "AWS_S3_BUCKET",
    "AWS_S3_ENDPOINT",
    "AWS_SECRET_ACCESS_KEY",
    "CARGO_BUILD_TARGET",
    "CARGO_TARGET_DIR",
    "CI",
    "DATABASE_URL",
    "DATABASE_URL_UNPOOLED",
    "DISABLE_PDF_SIGNING",
    "DOCUMENSO_DISABLE_TELEMETRY",
    "E2E_TEST_AUTHENTICATE_USER_EMAIL",
    "E2E_TEST_AUTHENTICATE_USER_PASSWORD",
    "E2E_TEST_AUTHENTICATE_USERNAME",
    "ENCRYPTION_KEY",
    "GOOGLE_APPLICATION_CREDENTIALS",
    "GOOGLE_VERTEX_API_KEY",
    "GOOGLE_VERTEX_LOCATION",
    "GOOGLE_VERTEX_PROJECT_ID",
    "INNGEST_EVENT_KEY",
    "JWT_SECRET",
    "NEXT_PRIVATE_DATABASE_URL",
    "NEXT_PRIVATE_DIRECT_DATABASE_URL",
    "NEXT_PRIVATE_ENCRYPTION_KEY",
    "NEXT_PRIVATE_ENCRYPTION_SECONDARY_KEY",
    "NEXT_PRIVATE_GITHUB_TOKEN",
    "NEXT_PRIVATE_GOOGLE_CLIENT_ID",
    "NEXT_PRIVATE_GOOGLE_CLIENT_SECRET",
    "NEXT_PRIVATE_INNGEST_APP_ID",
    "NEXT_PRIVATE_INNGEST_EVENT_KEY",
    "NEXT_PRIVATE_INTERNAL_WEBAPP_URL",
    "NEXT_PRIVATE_JOBS_PROVIDER",
    "NEXT_PRIVATE_LOGGER_FILE_PATH",
    "NEXT_PRIVATE_MAILCHANNELS_API_KEY",
    "NEXT_PRIVATE_MAILCHANNELS_DKIM_DOMAIN",
    "NEXT_PRIVATE_MAILCHANNELS_DKIM_PRIVATE_KEY",
    "NEXT_PRIVATE_MAILCHANNELS_DKIM_SELECTOR",
    "NEXT_PRIVATE_MAILCHANNELS_ENDPOINT",
    "NEXT_PRIVATE_OIDC_CLIENT_ID",
    "NEXT_PRIVATE_OIDC_CLIENT_SECRET",
    "NEXT_PRIVATE_OIDC_PROMPT",
    "NEXT_PRIVATE_OIDC_PROVIDER_LABEL",
    "NEXT_PRIVATE_OIDC_SKIP_VERIFY",
    "NEXT_PRIVATE_OIDC_WELL_KNOWN",
    "NEXT_PRIVATE_PLAIN_API_KEY",
    "NEXT_PRIVATE_RESEND_API_KEY",
    "NEXT_PRIVATE_SIGNING_GCLOUD_APPLICATION_CREDENTIALS_CONTENTS",
    "NEXT_PRIVATE_SIGNING_GCLOUD_HSM_KEY_PATH",
    "NEXT_PRIVATE_SIGNING_GCLOUD_HSM_PUBLIC_CRT_FILE_CONTENTS",
    "NEXT_PRIVATE_SIGNING_GCLOUD_HSM_PUBLIC_CRT_FILE_PATH",
    "NEXT_PRIVATE_SIGNING_LOCAL_FILE_CONTENTS",
    "NEXT_PRIVATE_SIGNING_LOCAL_FILE_PATH",
    "NEXT_PRIVATE_SIGNING_PASSPHRASE",
    "NEXT_PRIVATE_SIGNING_TRANSPORT",
    "NEXT_PRIVATE_SMTP_APIKEY",
    "NEXT_PRIVATE_SMTP_APIKEY_USER",
    "NEXT_PRIVATE_SMTP_FROM_ADDRESS",
    "NEXT_PRIVATE_SMTP_FROM_NAME",
    "NEXT_PRIVATE_SMTP_HOST",
    "NEXT_PRIVATE_SMTP_PASSWORD",
    "NEXT_PRIVATE_SMTP_PORT",
    "NEXT_PRIVATE_SMTP_SECURE",
    "NEXT_PRIVATE_SMTP_SERVICE",
    "NEXT_PRIVATE_SMTP_TRANSPORT",
    "NEXT_PRIVATE_SMTP_UNSAFE_IGNORE_TLS",
    "NEXT_PRIVATE_SMTP_USERNAME",
    "NEXT_PRIVATE_STRIPE_API_KEY",
    "NEXT_PRIVATE_STRIPE_WEBHOOK_SECRET",
    "NEXT_PRIVATE_TELEMETRY_HOST",
    "NEXT_PRIVATE_TELEMETRY_KEY",
    "NEXT_PRIVATE_UPLOAD_ACCESS_KEY_ID",
    "NEXT_PRIVATE_UPLOAD_BUCKET",
    "NEXT_PRIVATE_UPLOAD_DISTRIBUTION_DOMAIN",
    "NEXT_PRIVATE_UPLOAD_DISTRIBUTION_KEY_CONTENTS",
    "NEXT_PRIVATE_UPLOAD_DISTRIBUTION_KEY_ID",
    "NEXT_PRIVATE_UPLOAD_ENDPOINT",
    "NEXT_PRIVATE_UPLOAD_FORCE_PATH_STYLE",
    "NEXT_PRIVATE_UPLOAD_REGION",
    "NEXT_PRIVATE_UPLOAD_SECRET_ACCESS_KEY",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_DISABLE_SIGNUP",
    "NEXT_PUBLIC_DOCS_URL",
    "NEXT_PUBLIC_DOCUMENT_SIZE_UPLOAD_LIMIT",
    "NEXT_PUBLIC_FEATURE_BILLING_ENABLED",
    "NEXT_PUBLIC_MARKETING_URL",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "NEXT_PUBLIC_PROJECT",
    "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_UPLOAD_TRANSPORT",
    "NEXT_PUBLIC_WEBAPP_URL",
    "NEXTAUTH_SECRET",
    "NODE_ENV",
    "PDF_EXTERNAL_SERVICE_URL",
    "PDF_GENERATION_METHOD",
    "PORT",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL",
    "POSTGRES_URL_NON_POOLING",
    "RATE_LIMIT_ENABLED",
    "REDIS_URL",
    "RUSTFLAGS",
    "SESSION_SECRET",
    "SKIP_ENV_VALIDATION",
    "STORAGE_LOCAL_PATH",
    "STORAGE_PROVIDER",
    "WEBHOOK_SECRET"
  ]
}
