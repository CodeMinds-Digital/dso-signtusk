{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "apps/remix/build/client",
  "installCommand": "npm install --force",
  "framework": null,
  "functions": {
    "apps/remix/api/index.js": {
      "includeFiles": "apps/remix/build/**/*",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/site\\.webmanifest",
      "headers": {
        "Content-Type": "application/manifest+json",
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/fonts/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(favicon|apple-touch-icon|android-chrome).*\\.(png|ico)",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, immutable, max-age=31536000"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/apps/remix/api"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/remix/api"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ALLOWED_ORIGINS": "https://dso-signtusk.vercel.app,https://dso-signtusk-remix-vpsy-cnv81yn0e-codemindsdigitals-projects.vercel.app,https://dso-signtusk-remix-vpsy-pb01iyucs-codemindsdigitals-projects.vercel.app,https://dso-signtusk-remix-vpsy-769v5j5lr-codemindsdigitals-projects.vercel.app,https://dso-signtusk-remix-vpsy-8dk4scl0d-codemindsdigitals-projects.vercel.app,https://*.vercel.app"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "SKIP_ENV_VALIDATION": "true",
      "TURBO_TELEMETRY_DISABLED": "1",
      "SKIP_PATCHES": "true"
    }
  }
}
