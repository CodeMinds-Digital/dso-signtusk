{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "apps/remix/build/client",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "apps/remix/api/index.js": {
      "includeFiles": "apps/remix/build/**/*",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/apps/remix/api"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "SKIP_ENV_VALIDATION": "true",
      "TURBO_TELEMETRY_DISABLED": "1",
      "SKIP_PATCHES": "true"
    }
  }
}
