{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "apps/remix/build/client",
  "installCommand": "npm install --force",
  "framework": null,
  "functions": {
    "apps/remix/api/index.js": {
      "includeFiles": "apps/remix/build/**/*",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/site\\.webmanifest",
      "dest": "/apps/remix/public/site.webmanifest"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/apps/remix/public/fonts/$1"
    },
    {
      "src": "/(favicon|apple-touch-icon|android-chrome).*\\.(png|ico)",
      "dest": "/apps/remix/public/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|assets/|fonts/|static/|site\\.webmanifest|favicon|apple-touch-icon|android-chrome|robots\\.txt|\\.png|\\.jpg|\\.ico|\\.ttf|\\.woff|\\.woff2|\\.svg|\\.webp).*)",
      "destination": "/apps/remix/api"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/site.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/(favicon|apple-touch-icon|android-chrome).*\\.(png|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ALLOWED_ORIGINS": "https://dso-signtusk.vercel.app,https://dso-signtusk-remix-vpsy-cnv81yn0e-codemindsdigitals-projects.vercel.app,https://dso-signtusk-remix-vpsy-pb01iyucs-codemindsdigitals-projects.vercel.app,https://dso-signtusk-remix-vpsy-769v5j5lr-codemindsdigitals-projects.vercel.app,https://*.vercel.app"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "SKIP_ENV_VALIDATION": "true",
      "TURBO_TELEMETRY_DISABLED": "1",
      "SKIP_PATCHES": "true"
    }
  }
}
