    "color": "^4.2.3",
    "css-tree": "^2.3.1",
    "handlebars": "^4.7.8",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "fast-check": "^3.15.0"
