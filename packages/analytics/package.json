{
  "name": "@signtusk/analytics",
  "version": "0.1.0",
  "description": "Organization analytics and reporting system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --dts",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@signtusk/database": "*",
    "@signtusk/lib": "*",
    "@prisma/client": "^5.7.1",
    "zod": "^3.22.4",
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ]
}
